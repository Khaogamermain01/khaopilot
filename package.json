{
  "name": "khaopilot",
  "publisher": "khaogamermain01",
  "displayName": "KhaoPilot",
  "description": "GitHub Copilot in 83 lines of JavaScript.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "AI"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "./icon.png",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "khaopilot.token",
        "title": "KhaoPilot: Set OpenAI API Base URL, Key, and Model"
      }
    ],
    "configuration": [
      {
        "title": "KhaoPilot",
        "properties": {
          "picopilot.prompt": {
            "type": "string",
            "default": "",
            "description": "The prompt to use to generate completions. '{language}' is replaced with the active document language."
          }
        }
      }
    ]
  },
  "scripts": {
    "build": "webpack --mode production --devtool hidden-source-map",
    "watch": "bun run build -- --watch",
    "package": "vsce package --allow-star-activation"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "openai": "^4.57.0"
  }
}